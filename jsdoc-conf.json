{
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
      "plugins": ["transform-flow-strip-types"]
  },
  "source": {
    "include": ["./package.json", "./README.md"],
    "exclude": [ /* array of paths to exclude */ ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true
  }
}